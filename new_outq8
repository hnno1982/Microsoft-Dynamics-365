 //display icon and tooltio for the grid column  
 function outq8(rowData, userLCID) {  
   var str = JSON.parse(rowData);  
   var coldata = str.new_countryofresidence_Value;  
   var imgName = "";  
   var tooltip = "";  
   switch (coldata) {  
     case 100000000:  
       imgName = "new_/images/q8.gif";  
       tooltip = "Inside Kuwait";  
       break;      
   }  
   var resultarray = [imgName, tooltip];  
   return resultarray;  
 }  
 